{
  "name": "react-currency-control",
  "version": "1.0.6",
  "description": "React Input Control to handle and apply currency masks in your own inputs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Leonardo Dias",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push origin main && git push origin main --tags"
  },
  "dependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.1",
    "prettier": "^2.8.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "currency",
    "mask",
    "brl",
    "input",
    "money"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leoreisdias/react-currency-control.git"
  },
  "homepage": "https://github.com/leoreisdias/react-currency-control#readme",
  "bugs": {
    "url": "https://github.com/leoreisdias/react-currency-control/issues"
  }
}
